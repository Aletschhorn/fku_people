<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<T3DataStructure>
	<meta>
		<langDisable>1</langDisable>
	</meta>
	<sheets>
		<sDEF>
			<ROOT>
				<TCEforms>
					<sheetTitle>Settings</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<settings.teams>
						<TCEforms>
							<label>Subteam IDs (comma-separated)</label>
							<config>
								<type>input</type>
								<size>5</size>
							</config>
						</TCEforms>
					</settings.teams>
					<settings.display>
						<TCEforms>
							<label>Displayed information</label>
							<config>
								<type>select</type>
								<renderType>selectCheckBox</renderType>
								<items type="array">
									<numIndex index="0" type="array">
										<numIndex index="0">Name</numIndex>
										<numIndex index="1">name</numIndex>
									</numIndex>
									<numIndex index="1" type="array">
										<numIndex index="0">Address</numIndex>
										<numIndex index="1">address</numIndex>
									</numIndex>
									<numIndex index="2" type="array">
										<numIndex index="0">Phone</numIndex>
										<numIndex index="1">phone</numIndex>
									</numIndex>
									<numIndex index="3" type="array">
										<numIndex index="0">E-mail</numIndex>
										<numIndex index="1">mail</numIndex>
									</numIndex>
									<numIndex index="4" type="array">
										<numIndex index="0">Birthday</numIndex>
										<numIndex index="1">birthday</numIndex>
									</numIndex>
								</items>
								<maxitems>5</maxitems>
								<size>5</size>
							</config>
						</TCEforms>
					</settings.display>
				</el>
			</ROOT>
		</sDEF>
	</sheets>
</T3DataStructure>